import java.util.Arrays;
import java.util.Scanner;

public class Today {

	public static void main(String[] args) {
		
		int[] arr= {30,20,50,40,10};
		
		for(int i=4; i>arr.length;i--) {
			if(arr[i]>arr[i-1]) {
				int tmp = arr[i];
				arr[i] = arr[i-1];
				arr[i-1]= tmp;
			}
		}
		System.out.println(Arrays.toString(arr));
	
	}

}
