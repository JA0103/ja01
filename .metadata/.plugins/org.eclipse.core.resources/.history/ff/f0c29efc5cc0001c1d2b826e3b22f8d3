import java.util.Arrays;
import java.util.Scanner;

public class Today {

	public static void main(String[] args) {
		
		int[] arr= {9,6,7,3,5};
		
		for(int i=0; i<arr.length-1;i++) {
			for(int j =1; j<arr.length-1;j++) {
				
				if(arr[i]>arr[j-i]) {
					int tmp = arr[i];
					arr[i] = arr[j-i];
					arr[j-i]= tmp;
					System.out.println(Arrays.toString(arr));
					
				}
			}
		}
	//	System.out.println(Arrays.toString(arr));
	
	}

}

