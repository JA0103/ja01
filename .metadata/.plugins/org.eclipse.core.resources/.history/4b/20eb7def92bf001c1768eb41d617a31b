import java.util.Arrays;

public class Ex02 {

	public static void main(String[] args) {
		//10개 숫자 랜덤 엽력 받아서 나온 숫자의 개수 세기
		
		int arr[] = new int [10];
		int cnt[] = new int [10];
		
		for(int i=0; i<arr.length;i++) {
			System.out.print(arr[i]= (int)(Math.random()*10));
		}
		for(int i = 0; i< arr.length;i++) {
			cnt[arr[i]]++;
		}
		for(int i=0; i<arr.length;i++) {
			System.out.println(i+"의 개수 : "+cnt[i]);
		}
	}//class.end

}
