import java.util.Arrays;

public class Ex01 {

	public static void main(String[] args) {
		//다섯 학생의 세 과목 점수를 더해 각 학생의 총점과 평균을 계산하고
		//과목별 총점을 계산하는 예제
		
		int[][] score= {
				//kr,eng,math
				{100,100,100}
				,{20,20,20}
				,{30,30,30}
				,{40,40,40}
				,{50,50,50}
		};
	
		int korTotal=0, engTotal=0, mathTotal=0;
		int sum=0;

		//합
		for(int i=0;i<score.length;i++) {
			
			korTotal += score[i][0]; 
			engTotal += score[i][0]; 
			mathTotal += score[i][0]; 
		}
		}//class.end

}
