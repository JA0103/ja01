import java.util.Arrays;
import java.util.Scanner;

public class Today1 {

	public static void main(String[] args) {
		
		
		Scanner sc = new Scanner(System.in);
		
		int age = sc.nextInt();
		
		int fee = add(age);
		
		System.out.println(fee+"ì›");
		
	}//calss.end

	
		static int add(int age) {
		int money = 2500;
		
		if(0<=age && age<=3)
			money =0;
		else if(age<=13)
			money-=money*0.5;
		else if(age<=19)
			money-=money*0.25;
		else if(age<=64)
			money= 2500;
		else if(age>=65)
			money=0;
		
		return money;
	}
}

