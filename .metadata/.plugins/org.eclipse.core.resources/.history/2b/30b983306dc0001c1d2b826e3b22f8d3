import java.util.Arrays;
import java.util.Scanner;

public class Today {

	public static void main(String[] args) {
		
		
		Scanner sc = new Scanner(System.in);
		
		int age = sc.nextInt();
		
		int fee = add(age);
		
		System.out.println(fee+"원");
		
	}//calss.end

	
	public static int add(int age) {
		int money = 2500;
		
		if(0<=age && age<=3)
			money =0;
		
		return money;
	}
}

