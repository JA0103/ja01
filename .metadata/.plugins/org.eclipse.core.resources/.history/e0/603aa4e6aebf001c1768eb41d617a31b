import java.util.Arrays;

public class Ex01 {

	public static void main(String[] args) {
	
		int[][] arr= {
				{5,5,5,5,5},
				{10,10,10,10,10},
				{20,20,20,20,20},
				{30,30,30,30,30}
		};
		
		int total=0;
		float avg=0;
		
		for(int[] tmp : arr) {
			for(int i : tmp) {
				total+=i;
			}
		}
		
		avg= total/arr.length;
		
		System.out.println("total : "+total);
		
		System.out.println("avg : "+avg);
	}//class.end
	

}
