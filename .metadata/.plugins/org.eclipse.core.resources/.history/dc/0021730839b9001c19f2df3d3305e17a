import java.util.Scanner;

public class Ex01 {

	public static void main(String[] args) {

		int num;
		int sum = 0;
		boolean i = true;
		
		
		
		Scanner sc = new Scanner(System.in);
		num = sc.nextInt();
		
		while (i) {
			if (num!=0) {
				sum +=num;
			}
			else {
				i =false;
			}//while end
			System.out.println("합계 : " +sum);
		}
	}//main의 끝

}
