import java.util.Scanner;

public class Ex02 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int num=0;
		int max=0;
		String tmp;
		
		
		System.out.println("정수 5회 입력");
		
		for(int i =0; i<5; i++) {
			tmp = sc.nextLine();
			num = Integer.parseInt(tmp);
			
			if (max < num) max = num;
		}
		
		System.out.println("최대값 : " +max);
	}

}
